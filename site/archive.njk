---
title: Archive
subtitle: Every single post on the site.
permalink: /archive/
eleventyExcludeFromCollections: true
subscribe:
    atom: "/feed.xml"
    json: "/feed.json"
    email: "https://buttondown.email/v5.chriskrycho.com"

---

{% extends 'base.njk' %}
{% import 'components/item.njk' as item %}
{% from 'components/archive-list.njk' import list %}

{%- block main -%}
<section class='content-block'>
{{item.header(type='archive', title=title, subtitle=subtitle, subscribe=subscribe)}}

{%- if collections.all.length -%}
   {% for entry in collections.all %}
      {{entry.title}}
   {% endfor %}
   {{ list(collections.all | archiveByYears('NEW_FIRST'), idByCollection=true)}}
{%- else -%}
   {%- include 'blocks/soon.njk' -%}
{%- endif -%}
</div>
{%- endblock main -%}
